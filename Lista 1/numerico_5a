%Questão 5.a

x = -5:0.001:5
y = 3.*x.*e.^(-2.*x.^(2))-2.*cos(x)-1; %Função f(x) = cos(3x-7)+0.59
plot(x,y, 'r-'); %Plot do gráfico, com o traço continuo na cor vermelha
xlim([-5 5]); %limitando os valores [-5,5] do eixo x
title('Gráfico f(x) = cos(3x-7)+0.59 com intervalor de [-5,5]'); %Titulo 
xlabel('Eixo x'); %label 
ylabel('Eixo y');
legend('f(x)');
grid on;